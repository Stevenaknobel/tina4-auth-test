<form id="monitoringForm" enctype="multipart/form-data" xmlns="http://www.w3.org/1999/html">
    <div class="row">
                    <div class="form-group col-md-6">
                <label for="monitoringId">Monitoring Id</label>
                <input class="form-control" type="text" id="monitoringId"
                       name="monitoringId" placeholder="Monitoring Id"
                       value="{{ data.monitoringId }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="siteId">Site Id</label>
                <input class="form-control" type="text" id="siteId"
                       name="siteId" placeholder="Site Id"
                       value="{{ data.siteId }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="typeId">Type Id</label>
                <input class="form-control" type="text" id="typeId"
                       name="typeId" placeholder="Type Id"
                       value="{{ data.typeId }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="status">Status</label>
                <input class="form-control" type="text" id="status"
                       name="status" placeholder="Status"
                       value="{{ data.status }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="createdAt">Created At</label>
                <input class="form-control" type="text" id="createdAt"
                       name="createdAt" placeholder="Created At"
                       value="{{ data.createdAt }}">
            </div>
            </div>
    {{"monitoringForm" | formToken | raw }}
</form>
