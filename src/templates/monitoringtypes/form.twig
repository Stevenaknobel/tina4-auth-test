<form id="monitoringTypesForm" enctype="multipart/form-data" xmlns="http://www.w3.org/1999/html">
    <div class="row">
                    <div class="form-group col-md-6">
                <label for="typeId">Type Id</label>
                <input class="form-control" type="text" id="typeId"
                       name="typeId" placeholder="Type Id"
                       value="{{ data.typeId }}">
            </div>
                    <div class="form-group col-md-6">
                <label for="typeName">Type Name</label>
                <input class="form-control" type="text" id="typeName"
                       name="typeName" placeholder="Type Name"
                       value="{{ data.typeName }}">
            </div>
            </div>
    {{"monitoringTypesForm" | formToken | raw }}
</form>
